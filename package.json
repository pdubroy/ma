{
  "name": "ma",
  "version": "0.1.0",
  "author": "Patrick Dubroy <pdubroy@gmail.com> (http://dubroy.com)",
  "license": "MIT",
  "dependencies": {
    "6to5": "^1.15.0",
    "immutable": "^3.0.2",
    "tree-walk": "^0.2.2"
  },
  "devDependencies": {
    "6to5ify": "^2.0.0",
    "beefy": "^2.1.1",
    "browserify": "^5.9.1",
    "dirwatch": "^1.1.0",
    "jshint": "^2.5.2",
    "tap-spec": "^1.0.1",
    "tape": "^2.13.4",
    "underscore": "^1.7.0",
    "virtual-dom": "0.0.21",
    "watchify": "^1.0.6"
  },
  "scripts": {
    "build": "browserify -d -i 6to5/register -s ma -o dist/ma-bundle.js .",
    "watch": "watchify -v -d -i 6to5/register -s ma -o dist/ma-bundle.js .",
    "demo": "beefy examples/calc/index.js -- -d -i 6to5/register",
    "test": "tape test/*.js | tap-spec",
    "test-continuous": "npm test; dirwatch *.js lib/*.js test/*.js -c 'which tput && tput clear; date; tape test/*.js' | tap-spec",
    "prepublish": "npm run build && npm run test && npm run lint",
    "lint": "jshint index.js lib test"
  },
  "description": "An experiment in loosely-coupled coordination of processes -- inspired by tuple spaces.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "browserify": {
    "transform": [ "6to5ify" ]
  }
}
